 .searchform {
  display: block;	
  margin: 0;
}

.searchform label, 
.searchform input {
  color: #737373;
  float: left;
  vertical-align: baseline;
}

.searchform label { margin: .125em .125em 0 0; }

.searchform input[type=search] {
  font: 1em/1.618 Open Sans, Arial, Sans-serif;
  border: .125em solid #FFAF0A;
  border-width: 0 0 3px;
  background-color: transparent;
  padding: .1875em .375em;
  width: 80%;
}

.searchform input[type=search]:focus {
  border-color: #B9062F;
  color: #ffffff;
  outline: 0;
}

@media only screen and (min-width: 48em) {
  .searchform input[type=search]{ width: 50%; }
}

/* Icon 2 */

#sidenav-icon {
  width: 60px;
  height: 45px;
  position: relative;
  z-index:2000;
 
  -webkit-transform: rotate(0deg);
  -moz-transform: rotate(0deg);
  -o-transform: rotate(0deg);
  transform: rotate(0deg);
  -webkit-transition: .5s ease-in-out;
  -moz-transition: .5s ease-in-out;
  -o-transition: .5s ease-in-out;
  transition: .5s ease-in-out;
  cursor: pointer;
	
}

#sidenav-icon span {
	
  display: inline;
  position: fixed;
   z-index:2000;
  height: 9px;
  width: 50%;
  background: #FFAF0A;
  opacity: 1;
  -webkit-transform: rotate(0deg);
  -moz-transform: rotate(0deg);
  -o-transform: rotate(0deg);
  transform: rotate(0deg);
  -webkit-transition: .25s ease-in-out;
  -moz-transition: .25s ease-in-out;
  -o-transition: .25s ease-in-out;
  transition: .25s ease-in-out;
 
}

#sidenav-icon span:nth-child(even) {
  left: 50%;
  border-radius: 0 9px 9px 0;
}

#sidenav-icon span:nth-child(odd) {
  left:0px;
  border-radius: 9px 0 0 9px;
}

#sidenav-icon span:nth-child(1), #sidenav-icon span:nth-child(2) {
  top: 0px;
}

#sidenav-icon span:nth-child(3), #sidenav-icon span:nth-child(4) {
  top: 18px;
}

#sidenav-icon span:nth-child(5), #sidenav-icon span:nth-child(6) {
  top: 36px;
}

#sidenav-icon.open span:nth-child(1),#sidenav-icon.open span:nth-child(6) {
  -webkit-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  transform: rotate(45deg);
}

#sidenav-icon.open span:nth-child(2),#sidenav-icon.open span:nth-child(5) {
  -webkit-transform: rotate(-45deg);
  -moz-transform: rotate(-45deg);
  -o-transform: rotate(-45deg);
  transform: rotate(-45deg);
}

#sidenav-icon.open span:nth-child(1) {
  left: 5px;
  top: 7px;
}

#sidenav-icon.open span:nth-child(2) {
  left: calc(50% - 5px);
  top: 7px;
}

#sidenav-icon.open span:nth-child(3) {
  left: -50%;
  opacity: 0;
}

#sidenav-icon.open span:nth-child(4) {
  left: 100%;
  opacity: 0;
}

#sidenav-icon.open span:nth-child(5) {
  left: 5px;
  top: 29px;
}

#sidenav-icon.open span:nth-child(6) {
  left: calc(50% - 5px);
  top: 29px;
}
