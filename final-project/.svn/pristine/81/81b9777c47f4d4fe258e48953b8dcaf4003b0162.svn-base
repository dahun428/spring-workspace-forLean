package com.sample.web.view;

import java.util.Date;
import java.util.List;

import com.sample.dto.PerformanceDto;
import com.sample.dto.UserPointHistoryDto;

public class User {
    // user main 영역
    private String id;
    private String password;
    private String nickname;
    private Date birthday;
    private String gender;
    private String email;
    private String tel;
    private boolean admin;

    // user sub 영역
    private Date regDate;
    private boolean deleted;
    private int point;
    private int getLike;
    private int getNotify;
    private String iconPath;
    private String grade;
    private int totalPoint;
    private boolean shutout;

    // user address영역
    private String postalCode;
    private String sido;
    private String sigungu;
    private String addr;

    // user coupon 및 badge영역
    private List<Coupon> coupons;
    private List<UserBadge> badges;
    private List<PerformanceDto> likes;
    
    // user interest 및 user intro
    private List<UserIntrest> interest;
    private List<String> intro;
    private List<UserPointHistoryDto> userPointHistory;
    
    public String getId() {
        return id;
    }

    public void setId(final String id) {
        this.id = id;
    }

    public String getPassword() {
        return password;
    }

    public void setPassword(final String password) {
        this.password = password;
    }

    public String getNickname() {
        return nickname;
    }

    public void setNickname(final String nickname) {
        this.nickname = nickname;
    }

 
	public String getGender() {
        return gender;
    }

    public void setGender(final String gender) {
        this.gender = gender;
    }

    public String getEmail() {
        return email;
    }

    public void setEmail(final String email) {
        this.email = email;
    }

    public String getTel() {
        return tel;
    }

    public void setTel(final String tel) {
        this.tel = tel;
    }

    public boolean isAdmin() {
        return admin;
    }

    public void setAdmin(final boolean admin) {
        this.admin = admin;
    }

    public Date getRegDate() {
        return regDate;
    }

    public void setRegDate(final Date regDate) {
        this.regDate = regDate;
    }

    public boolean isDeleted() {
        return deleted;
    }

    public void setDeleted(final boolean deleted) {
        this.deleted = deleted;
    }

    public int getPoint() {
        return point;
    }

    public void setPoint(final int point) {
        this.point = point;
    }

    public int getGetLike() {
        return getLike;
    }

    public void setGetLike(final int getLike) {
        this.getLike = getLike;
    }

    public int getGetNotify() {
        return getNotify;
    }

    public void setGetNotify(final int getNotify) {
        this.getNotify = getNotify;
    }

    public String getIconPath() {
        return iconPath;
    }

    public void setIconPath(final String iconPath) {
        this.iconPath = iconPath;
    }

    public String getGrade() {
        return grade;
    }

    public void setGrade(final String grade) {
        this.grade = grade;
    }

    public int getTotalPoint() {
        return totalPoint;
    }

    public void setTotalPoint(final int totalPoint) {
        this.totalPoint = totalPoint;
    }

    public boolean isShutout() {
        return shutout;
    }

    public void setShutout(final boolean shutout) {
        this.shutout = shutout;
    }

	 
 
	public Date getBirthday() {
		return birthday;
	}

	public void setBirthday(Date birthday) {
		this.birthday = birthday;
	}


	public String getPostalCode() {
		return postalCode;
	}

	public void setPostalCode(String postalCode) {
		this.postalCode = postalCode;
	}

	public String getSido() {
        return sido;
    }

    public void setSido(final String sido) {
        this.sido = sido;
    }

    public String getSigungu() {
        return sigungu;
    }

    public void setGungu(final String gungu) {
        this.sigungu = gungu;
    }

    public String getAddr() {
        return addr;
    }

    public void setAddr(final String addr) {
        this.addr = addr;
    }

    public List<Coupon> getCoupons() {
        return coupons;
    }

    public void setCoupons(final List<Coupon> coupons) {
        this.coupons = coupons;
    }

    public List<UserBadge> getBadges() {
        return badges;
    }

    public void setBadges(final List<UserBadge> badges) {
        this.badges = badges;
    }


	public List<String> getIntro() {
		return intro;
	}

	public void setIntro(List<String> intro) {
		this.intro = intro;
	}

	public void setSigungu(String sigungu) {
		this.sigungu = sigungu;
	}

	@Override
	public String toString() {
		return "User [id=" + id + ", password=" + password + ", nickname=" + nickname + ", birthday=" + birthday
				+ ", gender=" + gender + ", email=" + email + ", tel=" + tel + ", admin=" + admin + ", regDate="
				+ regDate + ", deleted=" + deleted + ", point=" + point + ", getLike=" + getLike + ", getNotify="
				+ getNotify + ", iconPath=" + iconPath + ", grade=" + grade + ", totalPoint=" + totalPoint
				+ ", shutout=" + shutout + ", postalCode=" + postalCode + ", sido=" + sido + ", sigungu=" + sigungu
				+ ", addr=" + addr + ", coupons=" + coupons + ", badges=" + badges + "]";
	}

	public List<PerformanceDto> getLikes() {
		return likes;
	}

	public void setLikes(List<PerformanceDto> likes) {
		this.likes = likes;
	}

	public List<UserIntrest> getInterest() {
		return interest;
	}

	public void setInterest(List<UserIntrest> interest) {
		this.interest = interest;
	}
	
	public List<UserPointHistoryDto> getUserPointHistory() {
		return userPointHistory;
	}
    public void setUserPointHistory(List<UserPointHistoryDto> userPointHistory) {
		this.userPointHistory = userPointHistory;
	}
    
}