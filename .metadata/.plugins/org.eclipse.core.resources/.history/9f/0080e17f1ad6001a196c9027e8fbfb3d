package kr.co.jhta.vo;

public class Delivery {
/*
 *  create table spring_oder_delivery(
        delivery_no number(10) primary key,
        delivery_username varchar(100) not null,
        delivery_tel varchar2(30) not null,
        delivery_address varchar2(500) not null,
        delivery_status varchar2(50) default '물품준비중',
        order_no number(10) references spring_orders(order_no)
    );
 * 
 */
	private long no;
	private String username;
	private String tel;
	private String address;
	private String status;
	private long orderNo;
	public Delivery() {
		// TODO Auto-generated constructor stub
	}
	long getNo() {
		return no;
	}
	void setNo(long no) {
		this.no = no;
	}
	String getUsername() {
		return username;
	}
	void setUsername(String username) {
		this.username = username;
	}
	String getTel() {
		return tel;
	}
	void setTel(String tel) {
		this.tel = tel;
	}
	String getAddress() {
		return address;
	}
	void setAddress(String address) {
		this.address = address;
	}
	String getStatus() {
		return status;
	}
	void setStatus(String status) {
		this.status = status;
	}
	long getOrderNo() {
		return orderNo;
	}
	void setOrderNo(long orderNo) {
		this.orderNo = orderNo;
	}
	@Override
	public String toString() {
		return "Delivery [no=" + no + ", username=" + username + ", tel=" + tel + ", address=" + address + ", status="
				+ status + ", orderNo=" + orderNo + "]";
	}
	
	
}
