<mapper namespace="kr.co.jhta.dao.BoardDao">
	<insert id="insertBoard" parameterType="Board">
		insert into spring_free_boards 
		( board_no, board_title, board_writer, board_password, board_content, board_filename )
		values
		( spring_board_seq.nextval, #{title}, #{writer}, #{password}, #{content}, #{filename} );
	</insert>
	<select id="selectBoards" parameterType="Board" resultType="Board"></select>
	<select id="selectBoard" parameterType="int" resultType="Board"></select>
	<update id="updateBoard" parameterType="Board"></update>
	<delete id="deleteBoard" parameterType="int"></delete>
	
</mapper>