package kr.co.jhta.vo;

import java.util.Date;

/*
 * BOOK_NO
BOOK_TITLE
BOOK_WRITE
BOOK_GENRE
BOOK_PUBLISHER
BOOK_PRICE
BOOK_DISCOUNT_PRICE
BOOK_REGISTERED_DATE
